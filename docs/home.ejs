<% if(blogs.length) { %>
  <div class="blog-container">
      <% blogs.forEach(blog => {  %>
      <div class="blog-card">
        <h1><%= blog.title %></h1>
        <p><%= blog.author %></p>
        <p><%= blog.review %></p>
        <form action="/blogs/<%= blog._id%>/delete" method="POST">
          <button class="blog-delete-btn" type="submit">Delete</button>
        </form>
      </div>
      <% }) %>
  </div>
  <% } else { %>
      <h3>No Blogs Found.</h3>
  <% } %>
  